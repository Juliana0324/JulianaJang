		//주사위 2개를 던져 합계가 짝수일때 : 짝! 출력
		//주사위 2개를 던져 합계가 홀수일때 : 홀! 출력
		//두 주사위의 값이 동일할 경우 : 더블 출력
		
		double rd1 = Math.random();
		int rDice1=(int)(rd1*6)+1;
		double rd2 = Math.random();
		int rDice2=(int)(rd2*6)+1;
		int rSum = rDice1+rDice2;
		if (rDice1==rDice2) {
			System.out.println("첫번째 주사위: "+rDice1+"\n"+"두번째 주사위: "+rDice2);
			System.out.println("더블");
		}		
		else if (rSum%2==0) {
			System.out.println("첫번째 주사위: "+rDice1+"\n"+"두번째 주사위: "+rDice2);
			System.out.println("짝!");
		}
		else if(rSum%2==1) {
			System.out.println("첫번째 주사위: "+rDice1+"\n"+"두번째 주사위: "+rDice2);
			System.out.println("홀!");			
		}
		
	}

