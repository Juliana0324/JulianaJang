//while 복습
//		int a=6;
//		while (a>5){
//			System.out.println("안녕");
//			a--;
//			
//		}
		int sum = 0 ;
		int i =1;
		while(i<=10) {
			sum += i;
			System.out.print("i는"+i);
			i++;
			System.out.println("일때 합은"+sum);
		}
		System.out.println("0~10까지의 합은"+sum);
	
	}
