let x = {name : 'Codeit'};
let y = x;

console.log(x);
console.log(y);

y.birth = 2017;
console.log(x);
console.log(y);
/*
실행결과
{ name: 'Codeit' }
{ name: 'Codeit' }
{ name: 'Codeit', birth: 2017 }
{ name: 'Codeit', birth: 2017 }
*/


